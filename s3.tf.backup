# create an S3 bucket to store the state file in

resource "aws_s3_bucket" "eureka-eks-terraform-state" {
    bucket = "eureka-eks-terraform-state"
 
    versioning {
      enabled = true
    }
 
    lifecycle {
      prevent_destroy = true
    }
 
    tags {
      Name = "S3 Remote Terraform State Store"
    }      
}